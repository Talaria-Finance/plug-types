{
    "name": "@nftchance/emporium-types",
    "version": "0.3.18",
    "license": "BUSL-1.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/nftchance/emporium-types.git"
    },
    "keywords": [
        "nftchance",
        "eip712",
        "typehash",
        "signatures",
        "eth",
        "ethereum",
        "solidity",
        "crypto",
        "intent",
        "intents",
        "emporium"
    ],
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "emporium": "dist/core/cli.mjs"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./*": {
            "import": "./dist/*.js",
            "require": "./dist/*.js"
        },
        "./zod": {
            "import": "./dist/zod/index.js",
            "require": "./dist/zod/index.js"
        },
        "./zod/*": {
            "import": "./dist/zod/*.js",
            "require": "./dist/zod/*.js"
        }
    },
    "scripts": {
        "prebuild": "pnpm lint && rm -rf ./dist && tsup && rm -rf ./zod.ts && pnpm emporium zod",
        "build": "tsup && pnpm emporium generate && pnpm emporium docs",
        "emporium": "node dist/core/cli.mjs",
        "lint": "tsc",
        "ci:release": "pnpm publish -r",
        "ci:version": "changeset version",
        "ci:publish": "changeset publish"
    },
    "files": [
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "abitype": "^0.9.9",
        "bundle-require": "^4.0.2",
        "commander": "^11.0.0",
        "dedent": "^1.5.1",
        "ethers": "^6.7.1",
        "find-up": "^6.3.0",
        "fs-extra": "^11.1.1",
        "pathe": "^1.1.1",
        "picocolors": "^1.0.0",
        "prettier": "^3.0.3",
        "solady": "^0.0.129",
        "tsup": "^7.2.0",
        "typescript": "^5.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "@types/dedent": "^0.7.0",
        "@types/fs-extra": "^11.0.2"
    }
}