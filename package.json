{
  "name": "@nftchance/emporium-types",
  "version": "0.4.1",
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/nftchance/emporium-types.git"
  },
  "keywords": [
    "nftchance",
    "eip712",
    "typehash",
    "signatures",
    "eth",
    "ethereum",
    "solidity",
    "crypto",
    "intent",
    "intents",
    "emporium"
  ],
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "bin": {
    "emporium": "dist/core/cli.js"
  },
  "exports": {
    "index": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "core/cli": {
      "types": "./dist/core/cli.d.ts",
      "default": "./dist/core/cli.js"
    },
    "zod": {
      "types": "./dist/zod/index.d.ts",
      "default": "./dist/zod/index.js"
    },
    "package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "DEV=true tsup",
    "build": "tsup && pnpm emporium generate && pnpm emporium docs",
    "prebuild": "pnpm lint && tsup && pnpm emporium zod",
    "emporium": "node dist/core/cli.js",
    "lint": "tsc",
    "ci:release": "pnpm publish -r",
    "ci:version": "changeset version",
    "ci:publish": "changeset publish"
  },
  "files": [
    "/index",
    "/core",
    "/zod",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "abitype": "^0.9.9",
    "bundle-require": "^4.0.2",
    "commander": "^11.0.0",
    "dedent": "^1.5.1",
    "ethers": "^6.7.1",
    "execa": "^8.0.1",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "pathe": "^1.1.1",
    "picocolors": "^1.0.0",
    "prettier": "^3.0.3",
    "solady": "^0.0.129",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.10"
  }
}
